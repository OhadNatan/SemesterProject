<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.WrongDateController">
   <children>
      <AnchorPane prefHeight="387.0" prefWidth="567.0">
         <children>
            <ImageView fx:id="ImgLogo" fitHeight="95.0" fitWidth="169.0" layoutX="200.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
            <Label layoutX="244.0" layoutY="102.0" text="Sorry">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="36.0" />
               </font>
            </Label>
            <Label layoutX="190.0" layoutY="172.0" text="But we full on this date">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="BtnBack" layoutX="28.0" layoutY="336.0" mnemonicParsing="false" onAction="#GoBack" text="Back" />
            <Label layoutX="101.0" layoutY="215.0" text="Please select an action to continue the process">
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="BtnWaitingList" layoutX="224.0" layoutY="256.0" mnemonicParsing="false" onAction="#OpenWaitingList" text="Join the waiting list" />
            <Button fx:id="BtnAlternativeDate" layoutX="181.0" layoutY="297.0" mnemonicParsing="false" onAction="#OpenAlternativeDate" text="Choose an alternative date and time" />
         </children>
      </AnchorPane>
   </children>
</VBox>
